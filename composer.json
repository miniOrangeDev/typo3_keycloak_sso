  "type": "typo3-cms-extension",
  "license": [
    "GPL-2.0-or-later"
  ],
  "require": {
    "typo3/cms-core": "^10. || ^11. || ^12."
  },
  "version": "1.0.0",
  "autoload": {
    "psr-4": {
      "Miniorange\\KeycloakSSO\\": "Classes/"
    }
